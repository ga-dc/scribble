<h1> EDIT POST </h1>


<%= form_for @post do |f| %>
  <%=f.label :title %>
  <%=f.text_field :title%>

  <%=f.label :text %>
  <%=f.text_field :text%>

  <%=f.label :photo_url %>
  <%=f.text_field :photo_url%>

  <%=f.submit "UPDATEX" %>

<% end %>


<%= form_for @post, method: :delete do |f| %>
  <%= f.submit "DELETEX" %>
<%end%>
