<h2>New Post</h2>

<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :post_image %>
  <%= f.text_field :post_image %>

  <%= f.label :content %>
  <%= f.text_field :content %>

  <%= f.submit %>

<% end %>
