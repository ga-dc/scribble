<h4> <a href="/posts"> < Home </a></h4>

<h2> Create Post </h2>

<%= form_for @post do |f| %>
  <p>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
</p>

<p>
  <%= f.label :user %><br>
  <%= f.text_field :user %>
</p>

<p class='contentFill'>
  <%= f.label :content %><br>
  <%= f.text_area :content %>
</p>

  <%= f.submit "Create" %>
<% end %>
