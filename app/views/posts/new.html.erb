<h2>New Post</h2>

<%= form_for @posts do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title%>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :body %>
  <%= f.text_field :body %>

  <%= f.label :image_url %>
  <%= f.text_field :image_url %>

  <%= f.submit "Create" %>
<% end %>
