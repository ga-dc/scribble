<h1> Create a New Post </h1>
<a href="/posts"> All Posts </a>

<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
<br>
  <%= f.label :author %>
  <%= f.text_field :author %>
<br>
  <%= f.label :content %>
  <%= f.text_field :content %>
<br>
  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>
<br>
  <%= f.label :link %>
  <%= f.text_field :link %>
<br>
  <%= f.submit "Post" %>
<% end %>
