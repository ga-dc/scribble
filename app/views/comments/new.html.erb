<h2>New Comment</h2>

<%= form_for @comment do |f| %>
  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :content %>
  <%= f.text_field :content %>

  <%= f.label :post_id %>
  <%= f.text_field :post_id %>

  <%= f.submit "Create" %>
<% end %>
